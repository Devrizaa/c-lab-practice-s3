#include <stdio.h>

int main()
{
    int i, n, a[i], j;
    printf ("Enter the no of elements?");
    scanf ("%d", &n);
    
    printf("Enter the %d elements:",n);
    scanf ("%d", &a[i]);
    for(i=0;i<n;++i)
    
    printf ("Enter the key:");
    scanf ("%d", &j);
    for (i=0;i<n;++i)
        if (a[i]==j)
        break;
            if (i<n)
            printf ("element found at %d", i);
            else
            printf("Element not found");
            return 0;
            
}